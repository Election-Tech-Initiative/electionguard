{
  "$schema": "http://json-schema.org/draft-07/schema",
  "title": "Encryption Device",
  "description": "Metadata for encryption device",
  "$ref": "#/definitions/EncryptionDevice",
  "definitions": {
    "EncryptionDevice": {
      "title": "EncryptionDevice",
      "type": "object",
      "properties": {
        "device_id": {
          "title": "Unique identifier for device",
          "type": "integer"
        },
        "session_id": {
          "title": "Used to identify session and protect the timestamp",
          "type": "integer"
        },
        "launch_code": {
          "title": "Election initialization value",
          "type": "integer"
        },
        "location": {
          "title": "Arbitrary string to designate the location of device",
          "type": "string"
        }
      },
      "required": ["device_id", "session_id", "launch_code", "location"]
    }
  }
}
